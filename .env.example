APP_NAME="Laravel Jetstream Boilerplate"
APP_VERSION="v1.0"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_PROTO="http"
APP_HOST="host10:8080"
APP_ROOT="/jetstream"
APP_URL="${APP_PROTO}://${APP_HOST}${APP_ROOT}"
APP_COPYRIGHT="Copyright Â© crackline.org 2024"

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
